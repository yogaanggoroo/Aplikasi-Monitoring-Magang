{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "object-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ]
  }
}